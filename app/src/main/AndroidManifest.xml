<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cocworking">


    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".CocworkingApp"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="MainActivity"></activity>
        <activity android:name=".AboutUsActivity"></activity>
        <activity android:name=".PiantinaActivity"></activity>
        <activity android:name=".PrimoPianoActivity"></activity>
        <activity android:name=".SecondoPianoActivity"></activity>
        <activity android:name=".ProfiloActivity"></activity>
        <activity android:name=".SalaRiunioniActivity"></activity>
        <activity android:name=".ConvenzioniActivity"></activity>
        <activity android:name=".PromoActivity"></activity>
        <activity android:name=".BachecaActivity"></activity>
        <activity android:name=".EventiActivity"></activity>
        <activity android:name=".SetupActivity"></activity>
        <activity android:name=".NotificheActivity"></activity>
        <activity android:name=".RegistrazioneActivity"></activity>
        <activity android:name="LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>